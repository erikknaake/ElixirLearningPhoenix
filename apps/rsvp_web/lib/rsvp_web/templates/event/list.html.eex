<h1>All events</h1>
<a href="<%= event_path(@conn, :create) %>" class="btn btn-primary pull-right">Add</a>
<%= render RsvpWeb.SharedView, "count.html", list: @events %>
<div class=list-group">
    <%= for e <- @events do %>
        <%= render "summary.html", event: e, conn: @conn %>
    <% end %>
</div>